<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="anchorPane_dashboard" xmlns="http://javafx.com/javafx/23.0.1"
   xmlns:fx="http://javafx.com/fxml/1"
   fx:controller="com.betsesportivas.Controllers.DashboardJogadoresController">
   <children>

      <MenuBar prefHeight="25.0" prefWidth="800.0">
         <menus>
            <Menu fx:id="menu_dashboard" mnemonicParsing="false" text="Competicoes">
               <items>
                  <MenuItem fx:id="menu_competicoes_dashboard" mnemonicParsing="false"
                     text="Dashboard" />
               </items>
            </Menu>
            <Menu fx:id="menu_jogadores1" mnemonicParsing="false" text="Categorias">
               <items>
                  <MenuItem fx:id="menu_categorias_dashboard" mnemonicParsing="false"
                     text="Dashboard" />
               </items>
            </Menu>
            <Menu fx:id="menu_atletas" mnemonicParsing="false" text="Atletas">
               <items>
                  <MenuItem fx:id="menu_atletas_dashboard" mnemonicParsing="false" text="Dashboard" />
               </items>
            </Menu>
            <Menu fx:id="menu_apostas" mnemonicParsing="false" text="Apostas">
               <items>
                  <MenuItem fx:id="menu_apostas_dashboard" mnemonicParsing="false" text="Dashboard" />
                  <MenuItem fx:id="menu_apostas_relatorio" mnemonicParsing="false" text="Relatório" />
               </items>
            </Menu>
            <Menu fx:id="menu_jogadores" mnemonicParsing="false" text="Jogadores">
               <items>
                  <MenuItem fx:id="menu_jogadores_dashboard" mnemonicParsing="false"
                     text="Dashboard" />
                  <MenuItem fx:id="menu_jogadores_relatorio" mnemonicParsing="false"
                     text="Relatório" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <Pane layoutY="25.0" prefHeight="575.0" prefWidth="800.0">
         <children>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="575.0" prefWidth="805.0"
               style="-fx-background-color: white;">
               <children>
                  <Label layoutX="364.0" layoutY="10.0"
                     style="-fx-font-size: 16; -fx-font-weight: bold;" text="Jogadores" />
                  <TableView fx:id="tableViewJogador" layoutY="40.0"
                     onMouseClicked="#abrirEdicaoJogador" prefHeight="489.0" prefWidth="805.0">
                     <columns>
                        <TableColumn fx:id="tableColumnNomeJogador" prefWidth="305.0"
                           style="-fx-font-size: 12;" text="Nome" />
                        <TableColumn fx:id="tableColumnEmailJogador" prefWidth="344.0"
                           style="-fx-font-size: 12;" text="Email" />
                        <TableColumn fx:id="tableColumnSaldoJogador" prefWidth="150.0"
                           style="-fx-font-size: 12;" text="Saldo" />
                     </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
                  <Button fx:id="btn_refresh" layoutX="762.0" layoutY="10.0" mnemonicParsing="false"
                     prefHeight="25.0" prefWidth="24.0" text="↺" />
                  <Button fx:id="btn_criarEvento" layoutX="14.0" layoutY="8.0" maxHeight="25.0"
                     maxWidth="25.0" minHeight="25.0" minWidth="25.0" mnemonicParsing="false"
                     onAction="#abrirPanelCriarJogador" prefHeight="25.0" prefWidth="25.0" text="+" />
                  <Button fx:id="btn_fazer_deposito" layoutX="50.0" layoutY="8.0" maxHeight="25.0"
                     minHeight="25.0" mnemonicParsing="false" onAction="#abrirPanelDeposito"
                     prefHeight="25.0" prefWidth="91.0" text="\$ depósito" textAlignment="CENTER" />
               </children>
            </AnchorPane>
         </children>
      </Pane>
      <Pane fx:id="pane_criar" layoutX="240.0" layoutY="92.0" prefHeight="280.0" prefWidth="285.0"
         style="-fx-background-color: rgb(210,210,210);" visible="false">
         <children>
            <Button fx:id="btn_pane_criar_fechar1" layoutX="247.0" layoutY="14.0"
               mnemonicParsing="false" onAction="#handleFecharCriar"
               style="-fx-background-color: rgb(187, 45, 59); -fx-background-color: hover: rgba(200, 12, 64, 1);"
               text="X" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Label layoutX="10.0" layoutY="96.0" text="Nome" />
            <TextField fx:id="textFieldNomeJogador" layoutX="125.0" layoutY="92.0" />
            <Label layoutX="12.0" layoutY="129.0" text="Email" />
            <TextField fx:id="textFieldEmailJogador" layoutX="124.0" layoutY="125.0" />
            <Button fx:id="btn_pane_criar_jogador" layoutX="80.0" layoutY="230.0"
               mnemonicParsing="false" onAction="#criarJogadorHandler" prefHeight="25.0"
               prefWidth="125.0" style="-fx-background-color: rgb(21, 115, 71);" text="Criar"
               textFill="WHITE" />
            <DatePicker fx:id="datePickerDataNascimento" layoutX="125.0" layoutY="160.0"
               prefHeight="25.0" prefWidth="149.0" />
            <Label layoutX="10.0" layoutY="164.0" text="Data Nascimento" />
            <Text layoutX="98.0" layoutY="32.0" strokeType="OUTSIDE" strokeWidth="0.0"
               text="Criar Jogador">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Text>
         </children>
      </Pane>
      <Pane fx:id="pane_editar" layoutX="250.0" layoutY="102.0" prefHeight="280.0" prefWidth="285.0"
         style="-fx-background-color: rgb(210,210,210);" visible="false">
         <children>
            <Button fx:id="btnEditarFechar" layoutX="247.0" layoutY="14.0" mnemonicParsing="false"
               onAction="#handleFecharEditar"
               style="-fx-background-color: rgb(187, 45, 59); -fx-background-color: hover: rgba(200, 12, 64, 1);"
               text="X" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Label layoutX="14.0" layoutY="96.0" text="Nome" />
            <TextField fx:id="textFieldEditarNomeJogador" layoutX="124.0" layoutY="92.0"
               prefHeight="25.0" prefWidth="149.0" />
            <Label layoutX="16.0" layoutY="129.0" text="Email" />
            <TextField fx:id="textFieldEditarEmailJogador" layoutX="124.0" layoutY="125.0" />
            <Button fx:id="btn_pane_editar_jogador" layoutX="80.0" layoutY="230.0"
               mnemonicParsing="false" onAction="#editarJogadorHandler" prefHeight="25.0"
               prefWidth="125.0" style="-fx-background-color: rgb(21, 115, 71);" text="Editar"
               textFill="WHITE" />
            <CheckBox fx:id="checkBoxEditarAtivoJogador" layoutX="118.0" layoutY="191.0"
               mnemonicParsing="false" text="Ativo" />
            <Label layoutX="16.0" layoutY="161.0" text="Saldo" />
            <Label fx:id="labelSaldoEditar" layoutX="125.0" layoutY="161.0" prefHeight="17.0"
               prefWidth="125.0" text="0.0" />
            <Text layoutX="94.0" layoutY="32.0" strokeType="OUTSIDE" strokeWidth="0.0"
               text="Editar Jogador">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Text>
         </children>
      </Pane>
      <Pane fx:id="pane_deposito" layoutX="160.0" layoutY="101.0" prefHeight="298.0"
         prefWidth="469.0" style="-fx-background-color: rgb(210,210,210);" visible="false">
         <children>
            <Button fx:id="btnDepositoFechar" layoutX="428.0" layoutY="14.0" mnemonicParsing="false"
               onAction="#handleFecharDeposito"
               style="-fx-background-color: rgb(187, 45, 59); -fx-background-color: hover: rgba(200, 12, 64, 1);"
               text="X" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Label layoutX="306.0" layoutY="67.0" text="Valor do depósito" />
            <TextField fx:id="valorDeposito" layoutX="306.0" layoutY="92.0" text="0.0" />
            <Button fx:id="buttonDepositarSaldo" layoutX="318.0" layoutY="251.0"
               mnemonicParsing="false" onAction="#depositarSaldoHandler" prefHeight="25.0"
               prefWidth="125.0" style="-fx-background-color: rgb(21, 115, 71);" text="Depositar"
               textFill="WHITE" />
            <TableView fx:id="tableViewJogadorDeposito" layoutX="14.0" layoutY="54.0"
               prefHeight="230.0" prefWidth="274.0">
               <columns>
                  <TableColumn fx:id="tableColumnNomeJogadorDeposito" prefWidth="168.0"
                     style="-fx-font-size: 12;" text="Nome" />
                  <TableColumn fx:id="tableColumnSaldoJogadorDeposito" prefWidth="104.0"
                     style="-fx-font-size: 12;" text="Saldo" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Text layoutX="177.0" layoutY="29.0" strokeType="OUTSIDE" strokeWidth="0.0"
               text="Realizar Depósito">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Text>
         </children>
      </Pane>
   </children>
</AnchorPane>